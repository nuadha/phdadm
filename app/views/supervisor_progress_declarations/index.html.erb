<h1>Listing supervisor_progress_declarations</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Scientific progress</th>
    <th>Following student plan</th>
    <th>Phd</th>
    <th>Supervisor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @supervisor_progress_declarations.each do |supervisor_progress_declaration| %>
  <tr>
    <td><%= supervisor_progress_declaration.date %></td>
    <td><%= supervisor_progress_declaration.scientific_progress %></td>
    <td><%= supervisor_progress_declaration.following_student_plan %></td>
    <td><%= User.find(supervisor_progress_declaration.phd_id).name %></td>
    <td><%= User.find(supervisor_progress_declaration.supervisor_id).name %></td>
    <td><%= link_to 'Show', supervisor_progress_declaration %></td>
    <td><%= link_to 'Edit', edit_supervisor_progress_declaration_path(supervisor_progress_declaration) %></td>
    <td><%= link_to 'Destroy', supervisor_progress_declaration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

