<!DOCTYPE html>
<html>
<head>
  <title>PhD Administration</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="banner">
		<%- # image_tag("logo.png") -%>
		<%= @page_title || "Phdadm" %>
	</div>
	<div id="columns">
		<div id="side">
			<% if false %>
			<p>This is a multiline comment</p>
			<%-# commented line -%>
			<div id="cart">
				<%= render @cart %>
			</div>
			<% end %>
			
			<% time = Time.now %>
			<%= time.to_formatted_s(:long)%>
			<br /><br />
			<%= link_to "Articles", articles_path %><br />
			<%= link_to "Authors", authors_path %><br />
			<%= link_to "Cources", courses_path %><br />
			<%= link_to "Enrollments", enrollments_path %><br />
			<%= link_to "Progresses", progresses_path %><br />
			<%= link_to "PhD Supervisor Relations", phd_supervisor_relations_path %><br />
			<%= link_to "Roles", roles_path %><br />
			<%= link_to "Users", users_path %><br />
		</div>
		<div id="main">
			<%= yield %>
		</div>
	</div>
</body>
</html>
